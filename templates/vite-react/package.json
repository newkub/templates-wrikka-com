{
  "name": "vite-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "cross-env NODE_ENV=production bun ./server.js",
    "build": "vite build",
    "build:remix": "remix vite:build",
    "format": "@biomejs/biome format --write",
    "lint": "@biomejs/biome lint --write && bunx oxlint --write-suggestions",
    "preview": "vite preview",
    "scan": "vite && bunx react-scan localhost:3000",
    "tunnel": "bunx untun http://localhost:3000"
  },
  "dependencies": {
    "@million/lint": "^1.0.14",
    "install": "^0.13.0",
    "motion": "^12.16.0",
    "nuqs": "^2.4.3",
    "ofetch": "^1.4.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-scan": "^0.3.4",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^3.25.51"
  },
  "devDependencies": {
    "@types/bun": "^1.2.15",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.1",
    "oxlint": "^0.16.12",
    "typescript": "~5.8.3",
    "unocss": "^66.1.3",
    "vite": "^6.3.5",
    "vite-plugin-pages": "^0.33.0"
  }
}
